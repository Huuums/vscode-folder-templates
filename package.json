{
  "name": "vscode-fast-folder-structure",
  "displayName": "Folder Templates",
  "description": "Create Templates for Folders and Files for easy reusability",
  "repository": {
    "type": "git",
    "url": "https://github.com/Huuums/vscode-folder-templates"
  },
  "keywords": [
    "Folder Creator",
    "File templates",
    "Folder Templates",
    "template",
    "Create Directories",
    "Productivity",
    "scaffold",
    "react",
    "angular",
    "vue"
  ],
  "version": "3.0.1",
  "publisher": "Huuums",
  "engines": {
    "vscode": "^1.37.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:FT.createFolderStructure",
    "onCommand:FT.openGlobalFolder"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "FT.createFolderStructure",
        "title": "Create New Templated Folder"
      },
      {
        "command": "FT.openGlobalFolder",
        "title": "Open Global Folder Templates Directory"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "FT.createFolderStructure",
          "group": "2_workspace@500"
        }
      ]
    },
    "configuration": {
      "title": "Folder Templates",
      "properties": {
        "folderTemplates.structures": {
          "type": "array",
          "scope": "window",
          "default": [],
          "description": "Configure as many structures as you would like."
        },
        "folderTemplates.fileTemplates": {
          "type": "object",
          "scope": "window",
          "default": {}
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/chai-fs": "^2.0.2",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/rimraf": "^3.0.0",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.3",
    "@types/vscode": "^1.37.0",
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "glob": "^7.1.4",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.0",
    "sinon-chai": "^3.5.0",
    "tslint": "^6.0.0",
    "typescript": "^4.1.2",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "@types/html-tags": "^3.0.1",
    "html-tags": "^3.1.0",
    "open": "^7.3.0"
  }
}
